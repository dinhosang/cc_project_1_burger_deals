<header class="edit_object_header">
  <h2>Current Eatory Details</h2>
  <p>Eatory Name: <%= @eatory.name %></p>
</header>
<hr/>
<form class="edit_object" action="/eatories/<%= @eatory.id %>" method="post">
  <h2>Edit Eatory Details:</h2>
  <br/>
  <label for="eatory_name">Eatory Name: </label>
  <br/>
  <input type="text" name="name" value="" id="eatory_name" placeholder="<%= @eatory.name %>" autocomplete="off" />
  <br/>
  <br/>
  <br/>
  <br/>
  <section class="return_button">
    <div class="empty_container">
    </div>
    <button type="submit" name="">Save New Details</button>
    <div class="empty_container">
    </div>
  </section>
  <br/>
</form>
<br/>
<hr/>
<br/>
<form class="delete_object" action="/eatories/<%= @eatory.id %>/delete" method="post">
  <h2>Delete Eatory</h2>
  <br/>
  <button type="submit" name="">Delete Eatory</button>
</form>
