<% if @changes == false %>
  <header class="new_object_header">
    <h2><%= @eatery.name %></h2>
    <br/>
    <hr/>
    <h2>No New Burgers Added</h2>
    <br/>
    <hr/>
  </header>
<% elsif @changes == nil %>
  <header class="new_object_header">
    <h2><%= @eatery.name %></h2>
    <br/>
    <hr/>
    <h2>No New Burgers Added</h2>
    <br/>
    <hr/>
  </header>
  <section class="new_object">
    <br/>
    <h3>Stock Details</h3>
    <hr/>
    <p>Currently No stock</p>
    <hr/>
  </section>
<% else %>
  <header class="new_object_header">
    <h2><%= @eatery.name %></h2>
    <br/>
    <hr/>
    <h2>New Stock Successfully Added</h2>
    <br/>
  </header>
  <hr/>
  <section class="new_object">
    <br/>
    <h3>New Stock Details</h3>
    <hr/>
    <% for burger in @changes %>
      <% if burger.name != "" %>
        <br/>
        <p>Burger Name: <%= burger.name %></p>
        <p>Burger Type: <%= burger.type %></p>
        <br/>
      <% else %>
        <br/>
        <p>Burger Type: <%= burger.type %></p>
        <br/>
      <% end %>
    <hr/>
    <% end %>
  </section>
<% end %>
<br>
<br>
<br>
<br>
