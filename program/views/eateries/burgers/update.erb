<form class="return_button" action="/eateries/<%= @eatery.id %>/burgers/edit" method="get">
  <div class="empty_container">
  </div>
  <button type="submit" name="button">Return to Modify Eatery Details</button>
  <div class="empty_container">
  </div>
</form>
